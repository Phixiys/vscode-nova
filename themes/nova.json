	"tokenColors": [
		{
			"settings": {
				"background": "#3C4C55",
				"foreground": "#C5D4DD"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D18EC2"
            }
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#899BA6"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#7FC1CA"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#7FC1CA"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#83AFE5"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#83AFE5"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#D18EC2"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#D18EC2"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#D18EC2"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D18EC2"
			}
		},
		{
			"scope": "beginning.punctuation.definition.quote.markdown",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "beginning.punctuation.definition.list.markdown",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#7FC1CA;"
			}
		},
		{
			"scope": "markup.heading.markdown",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#899BA6"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#A8CE93"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#7FC1CA"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#F2C38F"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#9A93E1"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#83AFE5"
			}
		},
		{
			"scope": "meta.object-literal.key",
			"settings": {
				"foreground": "#83AFE5"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#9A93E1"
			}
		},
		{
			"scope": "punctuation.definition.block",
			"name": "syntaxGroups.special",
			"settings": {
				"foreground": "#F2C38F"
			}
        },
        {
			"scope": "entity.name.tag.tsx",
			"settings": {
				"foreground": "#F2C38F"
			}
		},
        {
			"scope": "entity.other.attribute-name.tsx",
			"settings": {
				"foreground": "#83AFE5"
			}
		}
	]
}